<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_alignParentLeft="true"
    android:layout_alignParentTop="true"
    android:layout_marginLeft="0dp"
    android:layout_marginTop="1dp"
    android:fillViewport="true"
    android:isScrollContainer="true"
    android:orientation="vertical"
    android:padding="0dp"
    android:scrollbars="horizontal|vertical">

    <!--Linear para titulo-->
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="#ffe5e6eb"
        android:orientation="vertical">
        <!--Linear para titulo lessons-->
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#fff4f5ff"
            android:orientation="vertical">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">

                <TextView
                    android:id="@+id/tv_title_test"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:gravity="left|center"
                    android:text="Repasa con DevTest"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#ff050001"
                    android:layout_weight="1" />

                <Button
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="48dp"
                    android:layout_height="27dp"
                    android:text="MÁS"
                    android:id="@+id/btn_mas_test"
                    android:gravity="center"
                    android:textSize="10dp"
                    android:background="#ff69b0dc"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="15dp" />
            </LinearLayout>

            <TextView
                android:id="@+id/tv_sub_test"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text="Juega y repasa al mismo tiempo"
                android:textAppearance="?android:attr/textAppearanceSmall" />

        </LinearLayout>

        <HorizontalScrollView
            android:id="@+id/horizontalScrollViewtest"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center">

            <LinearLayout xmlns:card_view="http://schemas.android.com/apk/res-auto"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:paddingBottom="@dimen/activity_vertical_margin"
                android:paddingLeft="@dimen/activity_horizontal_margin"
                android:paddingRight="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_vertical_margin"
                tools:context=".MainActivity">
                <!--
                                    card_view:cardBackgroundColor="@android:color/holo_blue_light"-->
                <!--Curso 1 C++, cada card contiene 1 fondo, 1 titulo y una opción ver como subtitulo inferior
               TO DO: convertir en icono botón para acceder-->
                <android.support.v7.widget.CardView
                    android:id="@+id/card_test"
                    android:layout_width="180dp"
                    android:layout_height="270dp"
                    card_view:cardCornerRadius="6dp"
                    card_view:cardElevation="10dp"
                    card_view:cardPreventCornerOverlap="false"
                    card_view:cardUseCompatPadding="true"
                    android:layout_gravity="center">

                    <!--fondo del cardView-->
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="fitCenter"
                        android:src="@drawable/im_yellowtest"
                        android:paddingLeft="10dp"
                        android:paddingTop="30dp"
                        android:paddingRight="10dp"
                        android:paddingBottom="10dp" />
                    <!--Titulo curso-->
                    <TextView
                        android:id="@+id/txt_test"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="top"
                        android:padding="10dp"
                        android:text="@string/test"
                        android:textColor="#ff72b7e3"
                        android:textSize="16dp"
                        android:textStyle="bold"
                        android:background="#8c000000" />
                    <!--Subtitulo ver-->
                    <TextView
                        android:id="@+id/txt_ver_test"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:gravity="right"
                        android:padding="10dp"
                        android:text="Jugar"
                        android:textColor="#ff6cc4ef"
                        android:textSize="16dp"
                        android:textStyle="bold" />
                </android.support.v7.widget.CardView>




            </LinearLayout>

        </HorizontalScrollView>
        <!--Linear para titulo lessons-->
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#fff4f5ff"
            android:orientation="vertical">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">

                <TextView
                    android:id="@+id/tv_title_lesson"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:gravity="left|center"
                    android:text="Lessons"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textColor="#ff050001"
                    android:layout_weight="1" />

                <Button
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="48dp"
                    android:layout_height="27dp"
                    android:text="MÁS"
                    android:id="@+id/btn_mas_lessons"
                    android:gravity="center"
                    android:textSize="10dp"
                    android:background="#ff69b0dc"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="15dp" />
            </LinearLayout>

            <TextView
                android:id="@+id/tv_sub_lesson"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text="Acceso rápido a tus lecciones favoritas"
                android:textAppearance="?android:attr/textAppearanceSmall" />

        </LinearLayout>

