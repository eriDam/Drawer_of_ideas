                    android:layout_marginRight="15dp" />
            </LinearLayout>


            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text="Repositorios y ejemplos"
                android:textAppearance="?android:attr/textAppearanceSmall" />
        </LinearLayout>

        <!--Linear para onCode-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#ffe5e6eb"
            android:orientation="horizontal"
            android:paddingBottom="7dp"
            android:paddingLeft="5dp"
            android:paddingRight="5dp"
            android:paddingTop="7dp">


            <Button
                android:id="@+id/btn_oncode_git"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="3dp"
                android:background="@drawable/layers_git_button_bg"
                android:text="@string/git"
                android:textColor="#fffffdfb"
                android:textSize="10dp"
                android:elevation="10dp"
                android:elegantTextHeight="true"
                android:layout_weight="1"
                android:selectAllOnFocus="false" />
            <Button
                android:id="@+id/btn_oncode2"
                android:layout_width="140dp"
                android:layout_height="fill_parent"
                android:layout_margin="3dp"
                android:layout_weight="2"
                android:background="#fffffafa"
                android:text="@string/ejemplos"
                android:textColor="#ff34b5e5"
                android:textSize="10dp" />
        </LinearLayout>
        <!--linear para título gestión-->
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#fff4f5ff"
