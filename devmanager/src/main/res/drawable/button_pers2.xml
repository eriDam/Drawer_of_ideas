<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#ff54d066" />
            <stroke
                android:width="1dp"
                android:color="#ff27972e" />
            <corners
                android:radius="1dp" />

        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#ff92e09c"
                android:endColor="#ff02c21e"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#ff27972e" />
            <corners
                android:radius="1dp" />

        </shape>
    </item>
</selector>